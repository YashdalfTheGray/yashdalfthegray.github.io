{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": [false],
        "indent": [true, "spaces", 2],
        "object-literal-sort-keys": [false],
        "one-line": [true, "check-open-space", "check-whitespace"],
        "quotemark": [true, "single", "avoid-template", "avoid-escape"],
        "trailing-comma": [false]
    },
    "rulesDirectory": []
}
